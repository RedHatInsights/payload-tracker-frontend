    "axios": "0.19.0",
    "connected-react-router": "6.8.0",
    "history": "4.7.2",
    "luxon": "1.24.1",
    "react": "16.8.6",
    "react-csv": "1.1.1",
    "react-day-picker": "7.4.8",
    "react-dom": "16.8.6",
    "react-redux": "7.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-router-last-location": "2.0.1",
    "redux": "4.0.1",
